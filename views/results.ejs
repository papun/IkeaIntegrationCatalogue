<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <title> SearchResults</title>
  <link rel="stylesheet" href="./css/style.css">
  <meta name="viewport" content="width=device-width, initial-scale=1">


</head>

<body>
  <header>
    <div class="container">
      <div id="branding">
        <h1><span class="highlight">IKEA</span></h1>
      </div>
      <nav>
        <ul>
          <li class="current"><a href="/">Home</a></li>
          <li><a href="/about">About</a></li>
          <li><a href="/services">Contact Us</a></li>
        </ul>
      </nav>
    </div>
  </header>

  <section id="resultshowcase">
    <h2>Search Result for <%= JSON.stringify(data.name) %></h2>
    <table id="customers">
      <tr>
        <th>Key</th>
        <th>Value</th>
      </tr>
      <tr>
        <td>City Name</td>
        <td><%= JSON.stringify(data.name) %></td>
      </tr>
      <tr>
        <td>City Id</td>
        <td><%= JSON.stringify(data.id) %></td>
      </tr>
      <tr>
        <td>Lon/Lat</td>
        <td><%= JSON.stringify(data.coord.lon) %> / <%= JSON.stringify(data.coord.lat) %></td>
      </tr>
      <tr>
        <td>Temp</td>
        <td><%= JSON.stringify(data.main.temp) %></td>
      </tr>
      <tr>
        <td>Weather</td>
        <td><%= JSON.stringify(data.weather[0].description) %></td>
      </tr>
      <tr>
        <td>Country</td>
        <td><%= JSON.stringify(data.sys.country) %></td>
      </tr>
      <tr>
        <td>Wind Speed</td>
        <td><%= JSON.stringify(data.wind.speed) %></td>
      </tr>
      <tr>
        <td>Cloud</td>
        <td><%= JSON.stringify(data.clouds.all) %></td>
      </tr>
      <tr>
        <td>Sunrise</td>
        <td><%= JSON.stringify(data.sys.sunrise) %></td>
      </tr>
      <tr>
        <td>Sunset</td>
        <td><%= JSON.stringify(data.sys.sunset) %></td>
      </tr>
    </table>

  </section>


  <footer><%- include('partials/footer') %></footer>
</body>

</html>